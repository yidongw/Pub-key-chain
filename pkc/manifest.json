{
    "name": "Distribution of public keys with blockchain",
    "version": "0.1",
    "manifest_version": 2,
    "minimum_chrome_version": "29",
    "description": "Demonstration of the GMail API to export mails to PDF and insert them into drafts",
    "icons": {
        "16": "/images/Pdf_16x16_Crystal_SVG.png",
        "48": "/images/pdf-icon-48x48.png",
        "128": "/images/pdf-icon-128x128.png"
    },
    "background": {
        "scripts": [
            "background.js",
            "client.js",
            "lib/base64.min.js"
        ]
    },
    "key": "",
    "content_security_policy": "script-src 'self' https://apis.google.com; object-src 'self'",
    "browser_action": {
        "default_popup": "popup.html",
        "default_icon": {
            "19": "/images/Pdf_16x16_Crystal_SVG.png",
            "38": "/images/pdf-icon-48x48.png"
        }
    },
    "permissions": [
        "tabs",
        "identity",
        "https://*.google.com/*",
        "https://ssl.gstatic.com/",
        "https://www.googleapis.com/",
        "https://accounts.google.com/"
    ],
    "oauth2": {
        "client_id": "662022900345-q62j9fu2v95af4hfonlco4qtiq2oe6gv.apps.googleusercontent.com",
        "scopes": [
            "https://www.googleapis.com/auth/drive.file",
            "https://www.googleapis.com/auth/gmail.readonly",
            "https://www.googleapis.com/auth/gmail.compose"
        ]
    }
}
